{
  "version": 2,
  "installCommand": "npm install",
  "builds": [
    { "src": "dist/apps/front-end/**", "use": "@vercel/static-build" },
    { "src": "dist/apps/back-end/**", "use": "@vercel/node" },
    { "src": "dist/libs/**", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/front-end/(.*)", "dest": "/dist/apps/front-end/$1" },
    { "src": "/back-end/(.*)", "dest": "/dist/apps/back-end/$1" },
    { "src": "/libs/(.*)", "dest": "/dist/libs/$1" },
    { "src": "/(.*)", "dest": "/dist/apps/front-end/$1" }
  ]
}
